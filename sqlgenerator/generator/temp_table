id int(11) NOT NULL AUTO_INCREMENT COMMENT '主键',
apply_id   int(11) NOT NULL COMMENT '申请单id',
user_id  int(11) NOT NULL COMMENT '用户id',
user_name varchar(64) NOT NULL COMMENT '用户名字',
is_see_history  bool NOT NULL COMMENT '是否允许查看历史数据',
is_deleted bit(1) NOT NULL DEFAULT b'0' COMMENT '0:有效,1:删除',
create_user varchar(50) DEFAULT '' COMMENT '创建人',
create_time datetime NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
modify_user varchar(50) DEFAULT '' COMMENT '更新人',
modify_time datetime DEFAULT CURRENT_TIMESTAMP COMMENT '更新时间',
ts bigint(20) NOT NULL COMMENT '同步记录时的时间戳',