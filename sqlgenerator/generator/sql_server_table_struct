[TransactionNumber] int NOT NULL IDENTITY(1,1) ,主键
[ProcedureId] uniqueidentifier NOT NULL ,工序Id
[ProcedureNo] nvarchar(50) NOT NULL ,工序编号
[ProcedureName] nvarchar(50) NOT NULL ,工序名
[StartProcedure] int NOT NULL ,开始工序
[EndProcedure] int NOT NULL ,结束工序
[DaySpan] int NOT NULL ,时间跨度
[IsNeedAudit] bit NOT NULL ,是否需要审核
[GroupNo] nvarchar(50) NULL ,所属组
[NextGroupNo] nvarchar(50) NULL ,下一个所属组
[IsEnable] bit NOT NULL ,是否启用
[OrderType] int NOT NULL ,订单类型
[WorkIds] nvarchar(200) NULL ,岗位
[InDate] datetime NOT NULL ,创建时间
[InUser] nvarchar(50) NOT NULL ,创建人
[UpDate] datetime NOT NULL ,修改时间
[UpUser] nvarchar(50) NULL ,修改人
[ProcedureType] int NOT NULL ,是否有工作量
[DisplayStandard] int NULL ,
[DisplayModel] int NULL ,
[ts] bigint NULL ,
[CityId] int NOT NULL ,城市ID
[IsPicture] bit NOT NULL ,
[IsVideo] bit NOT NULL ,
[IsMustFinishFlag] int NULL ,
[CreatorId] bigint NOT NULL ,
[CreateTime] datetime NOT NULL ,
[UpdateId] bigint NOT NULL ,
[IsDel] bit NOT NULL ,是否删除
[UnitPrice] decimal(18,2) NULL ,
[Unit] int NULL ,
[IsWorkload] bit NULL ,
[UpdateTime] datetime NOT NULL ,
[WorkloadCalcFormula] varchar(100) NULL ,
[CategoryCode] varchar(100) NULL ,
[IsDismantle] bit NULL ,
[HasSubProcedure] bit NOT NULL ,